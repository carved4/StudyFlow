{
  "version": 2,
  "name": "studyflow-react",
  "framework": "vite",
  "buildCommand": "VITE_CACHE_BUST=$(date +%s) npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "^/assets/(.*)\\.js$",
      "headers": {
        "content-type": "application/javascript",
        "cache-control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "src": "^/assets/(.*)\\.css$",
      "headers": {
        "content-type": "text/css",
        "cache-control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
